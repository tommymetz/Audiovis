{
  "name": "audiovis",
  "version": "1.0.6",
  "type": "module",
  "dependencies": {
    "dat.gui": "^0.7.9",
    "express": "^4.22.1",
    "sass": "^1.97.2",
    "socket.io": "^4.8.3",
    "sortablejs": "^1.15.6",
    "stats.js": "^0.17.0",
    "three": "^0.145.0"
  },
  "scripts": {
    "playlist": "python processor/playlist.py",
